{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/agents/{agentId}/channels/{channelId}/getReply": {
            "post": {
                "description": "入参出参均与原始文档保持一致 https://demo.laiye.com:8083/chatbot-openapi/swagger-ui/#/ChannelReplyService/ChannelReplyService_GetReply",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "botAgent"
                ],
                "summary": "代理机器人Api",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "body"
                        }
                    }
                }
            }
        },
        "/flush": {
            "post": {
                "description": "刷新昨日、近7天、近30天、近90天的TopN缓存结果",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "开发测试"
                ],
                "summary": "手动刷新统计数据缓存",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "body"
                        }
                    }
                }
            }
        },
        "/stats": {
            "post": {
                "description": "点击200 Successful Response查看具体接口返回格式",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "botAgent"
                ],
                "summary": "获取知识点TopN",
                "parameters": [
                    {
                        "description": "控制参数",
                        "name": "n",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.CommonIntentRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/model.StatsResponse"
                            }
                        }
                    }
                }
            }
        },
        "/top": {
            "post": {
                "description": "点击200 Successful Response查看具体接口返回格式",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "开发测试"
                ],
                "summary": "按精确时间获取知识点TopN",
                "parameters": [
                    {
                        "description": " ",
                        "name": "n",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.StatsRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/model.StatsResponse"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "model.CommonIntentRequest": {
            "type": "object",
            "properties": {
                "duration": {
                    "description": "duration-查询的时间范围 枚举类型：0.昨天 1.过去7天 2.过去30天 3.过去90天 4.上周汇总 5.上月汇总\" enum:\"0,1,2,3,4,5",
                    "type": "integer"
                },
                "n": {
                    "description": "检索数量前n的intent信息，默认为3",
                    "type": "integer"
                }
            }
        },
        "model.StatsRequest": {
            "type": "object",
            "properties": {
                "end_time": {
                    "description": "检索范围的结束时间 unix时间戳",
                    "type": "integer"
                },
                "n": {
                    "description": "检索数量前n的intent信息，默认为3",
                    "type": "integer"
                },
                "start_time": {
                    "description": "检索范围的起始时间 unix时间戳",
                    "type": "integer"
                }
            }
        },
        "model.StatsResponse": {
            "type": "object",
            "properties": {
                "cnt": {
                    "description": "召回次数",
                    "type": "integer"
                },
                "intent_id": {
                    "description": "知识点唯一id",
                    "type": "string"
                },
                "intent_name": {
                    "description": "知识点最新名称",
                    "type": "string"
                }
            }
        }
    }
}